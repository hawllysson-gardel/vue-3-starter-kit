{
  "name": "vue-3-starter-kit",
  "version": "1.0.1",
  "author": "Hawllysson Gardel Queiroz Almeida",
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "src/**/*.+(js|vue)": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hawllysson-gardel/vue-3-starter-kit"
  },
  "scripts": {
    "dev": "vite",
    "dev-unit": "vitest --dir src",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "clear": "rm -rf ./node_modules ./coverage ./dist ./.husky/_",
    "lint": "eslint 'src/**/*.{js,vue,html,md}'",
    "prettier": "eslint --fix 'src/**/*.{js,vue,html,md}'",
    "unit": "vitest run --coverage --dir src",
    "release": "semantic-release",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@pinia/testing": "^0.0.14",
    "@quasar/extras": "^1.15.5",
    "@vitest/coverage-c8": "^0.26.0",
    "axios": "^1.2.1",
    "connect-history-api-fallback": "^2.0.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "pinia": "^2.0.23",
    "quasar": "^2.11.3",
    "serve-static": "^1.15.0",
    "vue": "3.2.41",
    "vue-formor": "2.0.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@iconify/json": "^2.1.155",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@quasar/vite-plugin": "^1.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/vue": "^6.6.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/test-utils": "^2.2.2",
    "autoprefixer": "^10.4.13",
    "coveralls": "3.1.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "^9.8.0",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.18",
    "postcss-extend-rule": "^4.0.0",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.8.1",
    "process-envify": "^2.0.0",
    "sass": "~1.32.13",
    "semantic-release": "^19.0.1",
    "tailwindcss": "^3.2.4",
    "unplugin-icons": "^0.14.14",
    "vite": "^3.2.2",
    "vitest": "^0.25.2"
  },
  "engines": {
    "node": "^16",
    "npm": ">= 8.19.1"
  }
}
