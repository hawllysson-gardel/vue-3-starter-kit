{
  "name": "vue-3-starter-kit",
  "version": "1.1.0",
  "author": "Hawllysson Gardel Queiroz Almeida",
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "src/**/*.+(js|vue)": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hawllysson-gardel/vue-3-starter-kit"
  },
  "scripts": {
    "dev": "vite",
    "dev-unit": "vitest --dir src",
    "build": "vite build",
    "preview": "vite preview",
    "clear": "rm -rf ./node_modules ./coverage ./dist ./.husky/_",
    "lint": "eslint 'src/**/*.{js,vue,html,md}'",
    "prettier": "eslint --fix 'src/**/*.{js,vue,html,md}'",
    "unit": "vitest run --coverage --dir src",
    "release": "semantic-release",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@pinia/testing": "^0.1.3",
    "@quasar/extras": "^1.16.5",
    "@vitest/coverage-v8": "^0.34.3",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.6",
    "quasar": "^2.12.5",
    "vue": "3.3.4",
    "vue-formor": "3.2.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/json": "^2.2.105",
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "@quasar/vite-plugin": "^1.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/forms": "^0.5.5",
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.15",
    "coveralls": "3.1.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^10.11.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.28",
    "postcss-extend-rule": "^4.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.2",
    "process-envify": "^2.0.0",
    "sass": "~1.32.13",
    "semantic-release": "^21.1.1",
    "tailwindcss": "^3.3.3",
    "unplugin-icons": "^0.16.5",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "engines": {
    "node": "~20.5.1",
    "npm": "~9.8.0"
  }
}
